{
  "name": "learniverse",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.clientDev next dev",
    "build": "next build",
    "buildDev": "cp -f .env.clientDev .env && next build",
    "buildProd": "cp -f .env.clientProd .env && next build",
    "start": "env-cmd -f .env.clientProd next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss -c icon",
    "standalone": "node server.js",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "ahooks": "^3.8.4",
    "antd": "^5.24.0",
    "autoprefixer": "^10.4.20",
    "brainless-token-manager": "^1.3.3",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cookies-next": "^4.3.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "jotai": "^2.10.3",
    "lucide-react": "^0.512.0",
    "next": "^15.0.4",
    "next-i18next": "^11.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-otp-input": "^3.1.1",
    "reactjs-signal": "^1.0.5",
    "sharp": "^0.30.7",
    "sonner": "^1.7.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "umi-request": "^1.4.0",
    "zod": "^3.25.50"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.26",
    "@next/bundle-analyzer": "^12.3.4",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "18.7.16",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "bumpp": "^9.8.1",
    "cross-env": "^7.0.3",
    "csvgtocss": "^0.1.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^13.5.7",
    "esno": "^4.8.0",
    "git-scm-hooks": "^0.0.11",
    "postcss": "^8.4.49",
    "prettier": "^2.8.8",
    "sass": "^1.79.6",
    "tailwindcss": "^3.4.16",
    "typescript": "4.8.3",
    "verify-commit-msg": "^0.0.14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
